{
    "Serilog": {
        "MinimumLevel": {
            "Override": {
                "UMedia": "Debug"
            }
        },
        "WriteTo": {
            "AsyncSink": {
                "Name": "Async",
                "Args": {
                    "configure": {
                        "ConsoleSink": {
                            "Name": "Console",
                            "Args": {
                                "formatter": "UMedia.WebAPI.Serilog.Formatters::CommonFormatter, UMedia.WebAPI"
                            }
                        }
                    }
                }
            }
        }
    }
}
